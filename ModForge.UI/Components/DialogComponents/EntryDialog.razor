<MudDialog Class="dialog-card">
    <DialogContent>
        <MudText Typo="Typo.h6" Class="pb-6 header-primary" Style="font-weight: bold;">
            @L[MessageKeys.EntryDialogTitle].Value
        </MudText>
        <MudText GutterBottom="true">
            @L[MessageKeys.EntryDialogLine1].Value
        </MudText>
        <MudText GutterBottom="true">
            @L[MessageKeys.EntryDialogLine2].Value
        </MudText>
        <MudText GutterBottom="true">
            @L[MessageKeys.EntryDialogLine3].Value
        </MudText>
        <MudText GutterBottom="true">
            @L[MessageKeys.EntryDialogLine4].Value
        </MudText>
        <MudText GutterBottom="true" Style="font-style: italic">
            @L[MessageKeys.EntryDialogLine5].Value
        </MudText>
        <MudText GutterBottom="true">
            @L[MessageKeys.EntryDialogLine6].Value
        </MudText>

    </DialogContent>
    <DialogActions>
        <MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick="Submit">@L[MessageKeys.EntryDialogButtonText].Value</MudButton>
    </DialogActions>
</MudDialog>