@page "/stormItems"

@if (StormService is not null)
{

	<MudGrid Style="height: 100%; margin: 0px;" Spacing="0">
		@foreach (var storm in StormService.GetStormDtos())
		{
			<MudItem xs="12" Class="pa-2" onclick="@(() => NavigateToItem(storm.Id))">
				<StormItem Storm="storm" />
			</MudItem>
		}
	</MudGrid>
}
